{
  "manifest_version": 3,
  "name": "QuizMe!",
  "version": "1.0",
  "description": "Generate quizzes from page content.",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["<all_urls>", "https://generativelanguage.googleapis.com/"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
      "default_popup": "popup.html",
      "default_title": "QuizMe!"
  },
  "icons": {
    "512": "icons/QuizMeIcon.png"
  }
}